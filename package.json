{
  "name": "mocha-test-async",
  "version": "1.0.0",
  "description": "Different ways to test asynchronous code with mocha.js",
  "main": "index.js",
  "scripts": {
    "test": "mocha config/index.js --compilers js:babel-register",
    "test:watch": "mocha config/index.js --compilers js:babel-register -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/velmyk/mocha-test-async.git"
  },
  "keywords": [
    "mocha.js",
    "unit",
    "testing",
    "async"
  ],
  "author": "velmyk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/velmyk/mocha-test-async/issues"
  },
  "homepage": "https://github.com/velmyk/mocha-test-async#readme",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "devDependencies": {
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.9.0",
    "chai": "3.5.0",
    "mocha": "2.5.3",
    "proxyquire": "1.7.10",
    "q": "1.4.1",
    "sinon": "1.17.4",
    "sinon-as-promised": "3.0.1",
    "sinon-chai": "2.8.0"
  }
}
